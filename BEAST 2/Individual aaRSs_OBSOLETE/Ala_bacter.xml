<run id="mcmc" spec="MCMC" chainLength="10000000" storeEvery="10000">
    <state id="state" storeEvery="10000">
        <stateNode id="Tree.t:Ala" spec="bacter.model.SimulatedACG" delta="1000.0" rho="0.0">
            <populationModel id="ConstantPopulation.0" spec="ConstantPopulation">
                <parameter id="RealParameter.0" name="popSize">1.0</parameter>
            </populationModel>
            <locus id="locus.Ala" spec="bacter.Locus">
                <alignment idref="Ala"/>
            </locus>
            <taxonset id="TaxonSet.1" spec="TaxonSet" alignment="@Ala"/>
        </stateNode>
        <parameter id="rho.t:Ala" name="stateNode">0.001</parameter>
        <parameter id="delta.t:Ala" name="stateNode">1000.0</parameter>
        <parameter id="gammaShape.s:Ala" name="stateNode">1.0</parameter>
        <parameter id="mutationRate.s:Ala" name="stateNode">1.0</parameter>
        <parameter id="popSize.t:Ala" name="stateNode">1.0</parameter>
    </state>

    <distribution id="posterior" spec="util.CompoundDistribution">
        <distribution id="prior" spec="util.CompoundDistribution">
            <distribution id="CoalescentConstant.t:Ala" spec="bacter.model.ACGCoalescent" delta="@delta.t:Ala" rho="@rho.t:Ala" tree="@Tree.t:Ala">
                <populationModel id="popModelConstant.t:Ala" spec="ConstantPopulation" popSize="@popSize.t:Ala"/>
            </distribution>
            <prior id="deltaPrior.t:Ala" name="distribution" x="@delta.t:Ala">
                <OneOnX id="OneOnX.1" name="distr"/>
            </prior>
            <prior id="GammaShapePrior.s:Ala" name="distribution" x="@gammaShape.s:Ala">
                <Exponential id="Exponential.0" name="distr">
                    <parameter id="RealParameter.1" lower="0.0" name="mean" upper="0.0">1.0</parameter>
                </Exponential>
            </prior>
            <prior id="rhoPrior.t:Ala" name="distribution" x="@rho.t:Ala">
                <OneOnX id="OneOnX.0" name="distr"/>
            </prior>
            <prior id="popSizePrior.t:Ala" name="distribution" x="@popSize.t:Ala">
                <OneOnX id="OneOnX.2" name="distr"/>
            </prior>
        </distribution>
        <distribution id="likelihood" spec="util.CompoundDistribution">
            <distribution id="treeLikelihood.Ala" spec="bacter.model.ACGLikelihood" data="@Ala" locus="@locus.Ala" tree="@Tree.t:Ala">
                <siteModel id="SiteModel.s:Ala" spec="SiteModel" gammaCategoryCount="4" mutationRate="@mutationRate.s:Ala" shape="@gammaShape.s:Ala">
                    <parameter id="proportionInvariant.s:Ala" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="WAG.s:Ala" spec="WAG"/>
                </siteModel>
                <branchRateModel id="StrictClock.c:Ala" spec="beast.evolution.branchratemodel.StrictClockModel">
                    <parameter id="clockRate.c:Ala" estimate="false" name="clock.rate">1.0</parameter>
                </branchRateModel>
            </distribution>
        </distribution>
    </distribution>

    <operator id="addRemove_popModelConstant.t:Ala" spec="bacter.operators.AddRemoveConversion" acg="@Tree.t:Ala" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" weight="10.0"/>

    <operator id="addRemoveDetour_popModelConstant.t:Ala" spec="bacter.operators.AddRemoveDetour" acg="@Tree.t:Ala" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" weight="10.0"/>

    <operator id="CFUniform_popModelConstant.t:Ala" spec="bacter.operators.CFUniform" acg="@Tree.t:Ala" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" rho="@rho.t:Ala" weight="10.0"/>

    <operator id="CFWB_popModelConstant.t:Ala" spec="bacter.operators.CFWilsonBalding" acg="@Tree.t:Ala" alpha="0.1" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" rho="@rho.t:Ala" weight="10.0"/>

    <operator id="CFCS_popModelConstant.t:Ala" spec="bacter.operators.CFConversionSwap" acg="@Tree.t:Ala" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" rho="@rho.t:Ala" weight="10.0"/>

    <operator id="CFSTS_popModelConstant.t:Ala" spec="bacter.operators.CFSubtreeSlide" acg="@Tree.t:Ala" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" rho="@rho.t:Ala" weight="10.0"/>

    <operator id="CFSTXnarrow_popModelConstant.t:Ala" spec="bacter.operators.CFSubtreeExchange" acg="@Tree.t:Ala" delta="@delta.t:Ala" populationModel="@popModelConstant.t:Ala" rho="@rho.t:Ala" weight="10.0"/>

    <operator id="CFSTXwide_popModelConstant.t:Ala" spec="bacter.operators.CFSubtreeExchange" acg="@Tree.t:Ala" delta="@delta.t:Ala" isNarrow="false" populationModel="@popModelConstant.t:Ala" rho="@rho.t:Ala" weight="10.0"/>

    <operator id="rhoScaler.t:Ala" spec="ScaleOperator" parameter="@rho.t:Ala" scaleFactor="0.8" weight="1.0"/>

    <operator id="deltaScaler.t:Ala" spec="ScaleOperator" parameter="@delta.t:Ala" scaleFactor="0.8" weight="1.0"/>

    <operator id="ACGupDown3.t:Ala" spec="bacter.operators.ACGScaler" acg="@Tree.t:Ala" scaleFactor="0.8" weight="1.0">
        <parameterInverse idref="rho.t:Ala"/>
    </operator>

    <operator id="ACGscale.t:Ala" spec="bacter.operators.ACGScaler" acg="@Tree.t:Ala" scaleFactor="0.8" weight="1.0"/>

    <operator id="addRemoveRedundant.t:Ala" spec="bacter.operators.AddRemoveRedundantConversion" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="CRswap.t:Ala" spec="bacter.operators.ConvertedRegionSwap" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="CRshift.t:Ala" spec="bacter.operators.ConvertedRegionShift" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="CRBshift.t:Ala" spec="bacter.operators.ConvertedRegionBoundaryShift" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="mergeSplit.t:Ala" spec="bacter.operators.MergeSplitConversion" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="CEhop.t:Ala" spec="bacter.operators.ConvertedEdgeHop" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="CEflip.t:Ala" spec="bacter.operators.ConvertedEdgeFlip" acg="@Tree.t:Ala" weight="1.0"/>

    <operator id="CEslide.t:Ala" spec="bacter.operators.ConvertedEdgeSlide" acg="@Tree.t:Ala" apertureSize="0.05" weight="1.0"/>

    <operator id="popSizeScaler.t:Ala" spec="ScaleOperator" parameter="@popSize.t:Ala" scaleFactor="0.8" weight="1.0"/>

    <operator id="gammaShapeScaler.s:Ala" spec="ScaleOperator" parameter="@gammaShape.s:Ala" scaleFactor="0.5" weight="0.1"/>

    <operator id="FixMeanMutationRatesOperator" spec="DeltaExchangeOperator" delta="0.75" weight="2.0">
        <parameter idref="mutationRate.s:Ala"/>
        <weightvector id="weightparameter" spec="parameter.IntegerParameter" estimate="false" lower="0" upper="0">104</weightvector>
    </operator>

    <logger id="tracelog" fileName="$(filebase).log" logEvery="1000">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
        <log id="ACGStatsLog.t:Ala" spec="bacter.util.ConversionGraphStatsLogger" acg="@Tree.t:Ala"/>
        <log idref="rho.t:Ala"/>
        <log idref="delta.t:Ala"/>
        <log idref="popSize.t:Ala"/>
        <log idref="gammaShape.s:Ala"/>
        <log idref="mutationRate.s:Ala"/>
    </logger>

    <logger id="screenlog" logEvery="1000">
        <log idref="posterior"/>
        <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
    </logger>

    <logger id="ACGlogger.t:Ala" fileName="$(filebase).$(tree).trees" logEvery="10000" mode="tree">
