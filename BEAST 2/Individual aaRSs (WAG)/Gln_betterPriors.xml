                <LogNormal id="LogNormalDistributionModel.1" name="distr">
                    <parameter id="RealParameter.6" estimate="false" name="M">1.0</parameter>
                    <parameter id="RealParameter.7" estimate="false" lower="0.0" name="S" upper="5.0">1.25</parameter>
