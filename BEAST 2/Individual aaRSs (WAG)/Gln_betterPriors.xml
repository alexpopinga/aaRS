                    <parameter id="RealParameter.8" estimate="false" name="M">1.0</parameter>
                    <parameter id="RealParameter.9" estimate="false" lower="0.0" name="S" upper="5.0">1.25</parameter>
                </LogNormal>
            </prior>
            <prior id="ucldStdevPrior.c:ClassI_FULL" name="distribution" x="@ucldStdev.c:ClassI_FULL">
